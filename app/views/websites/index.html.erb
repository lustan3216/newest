<div class="row">
  <div class="col s7">
    <%= link_to 'LOGOUT', destroy_user_session_path, method: :delete %>
    <div class="card">
      <div class="card-content">
        <create-card :websites="websites"></create-card>
      </div>
    </div>
  </div>

  <div class="col s5">
    <div id="websites">
      <template v-for="website in websites">
        <card :website="website"></card>
      </template>
    </div>
  </div>
</div>